<?xml version="1.0" encoding="utf-8"?>
<Dashboard CurrencyCulture="en-US">
  <Title Text="Dashboard" />
  <DataSources>
    <SqlDataSource ComponentName="dashboardSqlDataSource1">
      <Name>SQL Data Source 1</Name>
      <Connection Name="DefaultConnection" FromAppConfig="true" />
      <Query Type="CustomSqlQuery" Name="Query">
        <Sql>SELECT TOP (1000) [ID]
					,[Code]
					,[Name]
					,[FirstName]
					,[FamilyName]
					,[NationalNumber]
					,[DaytimeTel]
					,[BirthDate]
					,[EmploymentDate]
					,[LeavingDate]
					,[Note]
					,[ISActive]
					,[OrgUnitID]
					,[BDHRTypeID]
					,[Created]
					,[CreatorID]
					,[UpdaterID]
					,[Updated]
					FROM [NetParseh].[dbo].[Employee]</Sql>
      </Query>
      <ResultSchema>
        <DataSet Name="SQL Data Source 1">
          <View Name="Query">
            <Field Name="ID" Type="Guid" />
            <Field Name="Code" Type="String" />
            <Field Name="Name" Type="String" />
            <Field Name="FirstName" Type="String" />
            <Field Name="FamilyName" Type="String" />
            <Field Name="NationalNumber" Type="String" />
            <Field Name="DaytimeTel" Type="String" />
            <Field Name="BirthDate" Type="DateTime" />
            <Field Name="EmploymentDate" Type="DateTime" />
            <Field Name="LeavingDate" Type="DateTime" />
            <Field Name="Note" Type="String" />
            <Field Name="ISActive" Type="Boolean" />
            <Field Name="OrgUnitID" Type="Guid" />
            <Field Name="BDHRTypeID" Type="Guid" />
            <Field Name="Created" Type="DateTime" />
            <Field Name="CreatorID" Type="Guid" />
            <Field Name="UpdaterID" Type="Guid" />
            <Field Name="Updated" Type="DateTime" />
          </View>
        </DataSet>
      </ResultSchema>
      <ConnectionOptions CloseConnection="true" CommandTimeout="0" />
    </SqlDataSource>
  </DataSources>
  <Items>
    <Chart ComponentName="chartDashboardItem1" Name="Chart 1" DataSource="dashboardSqlDataSource1" DataMember="Query">
      <DataItems>
        <Measure DataMember="FamilyName" SummaryType="Count" UniqueName="DataItem0" />
        <Measure DataMember="FirstName" SummaryType="Count" UniqueName="DataItem1" />
        <Measure DataMember="Note" SummaryType="Count" UniqueName="DataItem2" />
        <Measure DataMember="Updated" SummaryType="Count" UniqueName="DataItem3" />
      </DataItems>
      <Panes>
        <Pane Name="Pane 1">
          <Series>
            <Simple>
              <Value UniqueName="DataItem0" />
            </Simple>
            <Simple>
              <Value UniqueName="DataItem1" />
            </Simple>
            <Simple>
              <Value UniqueName="DataItem2" />
            </Simple>
            <Simple>
              <Value UniqueName="DataItem3" />
            </Simple>
          </Series>
        </Pane>
      </Panes>
    </Chart>
    <Pie ComponentName="pieDashboardItem1" Name="Pies 1" DataSource="dashboardSqlDataSource1" DataMember="Query">
      <DataItems>
        <Measure DataMember="FirstName" SummaryType="Count" UniqueName="DataItem0" />
        <Measure DataMember="NationalNumber" SummaryType="Count" UniqueName="DataItem1" />
        <Measure DataMember="Created" SummaryType="Count" UniqueName="DataItem2" />
        <Measure DataMember="Updated" SummaryType="Count" UniqueName="DataItem3" />
        <Measure DataMember="EmploymentDate" SummaryType="Count" UniqueName="DataItem4" />
      </DataItems>
      <Values>
        <Value UniqueName="DataItem0" />
        <Value UniqueName="DataItem1" />
        <Value UniqueName="DataItem2" />
        <Value UniqueName="DataItem3" />
        <Value UniqueName="DataItem4" />
      </Values>
    </Pie>
  </Items>
  <LayoutTree>
    <LayoutGroup>
      <LayoutItem DashboardItem="chartDashboardItem1" Weight="0.5" />
      <LayoutItem DashboardItem="pieDashboardItem1" Weight="0.5" />
    </LayoutGroup>
  </LayoutTree>
</Dashboard>